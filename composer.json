{
    "name": "siapepfrance/askimet-php-client",
    "version": "1.0.0",
    "description": "Documentation of the Akismet API",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://openapi-generator.tech",
